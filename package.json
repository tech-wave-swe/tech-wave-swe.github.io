{
  "name": "requirements-tracker",
  "publisher": "tech-wave-swe",
  "description": "A Requirements Tracker Extension",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.60.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Requirements Tracker",
      "properties": {
        "reqTracker.model": {
          "type": "string",
          "enum": [
            "qwen2.5-coder:7b",
            "stable-code:3b",
            "codellama:7b",
            "deepseek-coder-v2:16b",
            "custom"
          ],
          "default": "qwen2.5-coder:7b",
          "description": "Ollama model to use",
          "order": 2
        },
        "reqTracker.custom-model": {
          "type": "string",
          "default": "",
          "description": "Custom model name",
          "order": 3
        },
        "reqTracker.embeddingModel": {
          "type": "string",
          "enum": [
            "mxbai-embed-large:latest",
            "nomic-embed-text:latest",
            "all-minilm:latest",
            "custom"
          ],
          "default": "nomic-embed-text:latest",
          "description": "Ollama model to use for embeddings",
          "order": 4
        },
        "reqTracker.custom-embeddingModel": {
          "type": "string",
          "default": "",
          "description": "Custom embedding model name",
          "order": 5
        },
        "reqTracker.maxResults": {
          "type": "number",
          "default": 5,
          "description": "Maximum number of results to return from vector search"
        },
        "reqTracker.temperature": {
          "type": "number",
          "default": 0.7,
          "description": "Temperature parameter for text generation"
        },
        "reqTracker.endpoint": {
          "type": "string",
          "default": "http://localhost:11434",
          "description": "Endpoint for Ollama service"
        },
        "reqTracker.chunkSize": {
          "type": "number",
          "default": 1000,
          "description": "Size of text chunks for document splitting"
        },
        "reqTracker.chunkOverlap": {
          "type": "number",
          "default": 200,
          "description": "Overlap between consecutive chunks"
        }
      }
    },
    "commands": [
      {
        "command": "requirements-tracker.openSidebar",
        "title": "ReqTracker: Open Requirements Tracker Sidebar"
      },
      {
        "command": "requirements-tracker.openSettings",
        "title": "ReqTracker: Open Settings"
      },
      {
        "command": "requirementsTracker.interrogateSelection",
        "title": "ReqTracker: Interrogate on selected code based on requirements",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "requirementsTracker.interrogateDocument",
        "title": "ReqTracker: Interrogate on entire document based on requirements",
        "icon": "$(book)"
      },
      {
        "command": "requirementsTracker.indexCurrentFile",
        "title": "ReqTracker: Index current file"
      },
      {
        "command": "requirementsTracker.indexWorkspace",
        "title": "ReqTracker: Index workspace"
      },
      {
        "command": "requirementsTracker.clearChatHistory",
        "title": "ReqTracker: Clear chat history"
      },
      {
        "command": "requirementsTracker.resetDatabase",
        "title": "ReqTracker: Reset Database"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "requirementsTracker",
          "title": "Requirements Tracker",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "requirementsTracker": [
        {
          "type": "webview",
          "id": "requirementsTracker.trackerView",
          "name": "Requirements"
        },
        {
          "type": "webview",
          "id": "requirementsTracker.chatView",
          "name": "Chat"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "command": "requirementsTracker.interrogateSelection",
          "group": "navigation",
          "when": "editorTextFocus"
        },
        {
          "command": "requirementsTracker.interrogateDocument",
          "group": "navigation",
          "when": "editorIsOpen"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "requirements-tracker-walkthrough",
        "title": "Welcome to Requirements Tracker",
        "description": "This is a an extension that helps you track requirements in your codebase using Ollama",
        "steps": [
          {
            "id": "overview",
            "title": "Overview",
            "description": "This extension provides a Requirements Tracker view that can be used to track requirements",
            "media": {
              "image": "./media/logo.svg",
              "altText": "Overview"
            }
          },
          {
            "id": "changesetting",
            "title": "Change Setting",
            "description": "In order for this extension to function you need to setup your Ollama enviroment here:\n[Change Setting](command:requirements-tracker.openSettings)",
            "media": {
              "image": "./media/logo.svg",
              "altText": "Change Setting"
            }
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint",
    "watch": "tsc -w -p ./",
    "test:jest": "jest",
    "test:coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test": "node ./out/test/runTest.js",
    "test:unit": "TEST_TYPE=unit node ./out/test/runTest.js",
    "test:integration": "TEST_TYPE=integration node ./out/test/runTest.js",
    "test:e2e": "TEST_TYPE=e2e node ./out/test/runTest.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.12.0",
    "@langchain/community": "^0.3.36",
    "@langchain/ollama": "0.2.0",
    "@vscode/codicons": "^0.0.36",
    "csv-parse": "^5.3.5",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "2.11.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@types/xml2js": "^0.4.11",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.8.4",
    "typescript-eslint": "8.15.0",
    "vscode-test": "^1.6.1"
  }
}
